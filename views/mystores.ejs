<link rel="stylesheet" href="/css/mystores.css">

<div class="card-columns">
<%  users.forEach(function(user){%>
<div class="card " style="width: 18rem;">
    
    <img class="card-img-top" src="/pictures/<%= user.image%>" alt="Card image cap">
    <div class="card-body">
      <h5 class="card-title"><%= user.storeName%></h5>
      <p class="card-text"><%= user.description%></p>
      <a href="#" class="btn btn-primary">Check Products</a><br>
      <a href="/products/addproducts" class="btn btn-primary">Add Products</a>
    </div>
    
</div>
<% }) %>  
</div>